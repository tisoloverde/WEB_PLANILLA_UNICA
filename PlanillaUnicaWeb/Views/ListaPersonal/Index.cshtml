                    var checkFichaLee = document.getElementById("colaboradorFicha_Rhcol_Lee");
                    if (response.data.Rhcol_Lee == 'S') {
                        checkFichaLee.checked = true;
                    }
                    var checkFichaEscribe = document.getElementById("colaboradorFicha_Rhcol_Escribe");
                    if (response.data.Rhcol_Escribe == 'S') {
                        checkFichaEscribe.checked = true;
                    }
                    var checkFichaLicencia = document.getElementById("switchFichaLicenciaConducir");
