@if (TempData["Estado"] != null)
{
    if (TempData["Estado"].ToString().Equals("ERROR"))
    {
        <script>
                Swal.fire({
                icon: "error",
                title: "Error",
                html: "@TempData["Mensaje"].ToString()",
                confirmButtonText: 'Aceptar',
                confirmButtonColor: '#e60012',
                })
        </script>
    }
    else
    {
        <script>
                Swal.fire({
                icon: "success",
                title: "Aviso",
                text: "@TempData["Mensaje"].ToString()",
                confirmButtonText: 'Aceptar',
                confirmButtonColor: '#e60012',
                })
        </script>
    }
}

<div class="card shadow">
