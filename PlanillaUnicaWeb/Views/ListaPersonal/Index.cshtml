        //EDIT Cuando seleccionamos objeto de regiones (debemos filtar provincias y limpiar comunas)
        $('#colaboradorEdit_Genreg_Id').change(function () {
            var regionId = $(this).val(); // Obtener el ID de la familia seleccionada
            var provinciasColaboradorList = @Html.Raw(Json.Encode(lstProvincia));
            // Limpiar el dropdown de Provincias y comunas
            $('#colaboradorEdit_Genpro_Id').empty();
            $('#colaboradorEdit_Genpro_Id').append($('<option>').val('').text('-- Seleccione Provincia --')); // Opción por defecto
            $('#colaboradorEdit_Gencom_Id').empty();
            $('#colaboradorEdit_Gencom_Id').append($('<option>').val('').text('-- Seleccione Comuna --')); // Opción por defecto
            // Si hay una region seleccionada
            if (regionId) {
                // Filtrar las provincias que pertenecen a la region seleccionada
                var filteredProvincias = provinciasColaboradorList.filter(function (item) {
                    return item.Genreg_Id == regionId; // Comparar con la region seleccionada
                });

                // Llenar el dropdown con las provincias filtradas
                $.each(filteredProvincias, function (index, item) {
                    $('#colaboradorEdit_Genpro_Id').append($('<option>').val(item.Genpro_Id).text(item.Genpro_Descripcion));
                });
            }
        });

